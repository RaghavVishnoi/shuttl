      $('.link').on('click', function(event){
        var $this = $(this);
        if($this.hasClass('clicked')){
          $this.removeClass('clicked');
        } else{
          $this.addClass('clicked');
        }
      });

$('.check-box li').click(function(){
  $(this).addClass("selected");
  
  
});
	
$('#sub').click(function(){
 	
var geocoder = new google.maps.Geocoder();
var geocoder1 = new google.maps.Geocoder();
	
	
var address = $('#pac-input').val();
var address1 = $('#pac-input1').val();
 
geocoder.geocode( { 'address': address}, function(results, status) {

  if (status == google.maps.GeocoderStatus.OK) {
     slat  = results[0].geometry.location.lat();
     slng = results[0].geometry.location.lng();
   } 
});

geocoder1.geocode( { 'address': address1}, function(results, status) {

  if (status == google.maps.GeocoderStatus.OK) {
    dlat = results[0].geometry.location.lat();
    dlng = results[0].geometry.location.lng();
   }
 });

   location.href = 'routes/find_route?s='+address+'&slat='+slat+'&slong='+slng+'&d='+address1+'&dlat='+dlat+'&dlong='+dlng
});


	
});
	
	
	
    </script>
	
